# Database Configuration
HEALTH_HOST=localhost
HEALTH_USER=your_database_username
HEALTH_PASSWORD=your_database_password
HEALTH_DATABASE=your_database_name

# Application Configuration
# HEALTH_BASE_PATH is only needed if running behind a reverse proxy that forwards requests
# with the base path stripped. Leave empty for normal deployments.
HEALTH_BASE_PATH=your_base_path_url

# Session Configuration
SESSION_SECRET=your_session_secret_key
